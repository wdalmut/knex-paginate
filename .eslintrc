{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "standard",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
      "camelcase": 0,
       "comma-dangle": ["error", {
          "arrays": "always-multiline",
          "objects": "always-multiline"
       }],
       "prefer-promise-reject-errors": 0
    }
}
